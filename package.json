{
  "name": "rudget-web",
  "version": "0.1.0",
  "description": "Web client for Rudget.",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server",
    "test:lint": "eslint . --ext ts,tsx,js,jsx",
    "test:commit": "concurrently \"yarn:test:lint\" \"yarn:test:typing\" \"yarn:test:e2e\"",
    "test:typing": "tsc --noEmit",
    "test:e2e": "start-server-and-test 3000 'cypress run --config-file test/config.json'",
    "test:cy": "cypress open --config-file test/config.json",
    "build": "webpack -p"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@reach/router": "^1.3.1",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-utilities": "^1.3.2",
    "classnames": "^2.2.6",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-spring": "^8.0.27"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.1.3",
    "@svgr/webpack": "^5.2.0",
    "@types/classnames": "^2.2.9",
    "@types/faker": "^4.1.10",
    "@types/reach__router": "^1.2.6",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "autoprefixer": "^9.7.4",
    "awesome-typescript-loader": "^5.2.1",
    "concurrently": "^5.0.2",
    "css-loader": "^3.4.2",
    "cypress": "^4.1.0",
    "eslint": "^6.7.2",
    "eslint-config-zyehex": "^0.2.3",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-cypress": "^2.10.3",
    "faker": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "start-server-and-test": "^1.10.8",
    "style-loader": "^1.1.3",
    "typescript": "^3.7.2",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test:commit"
    }
  }
}
